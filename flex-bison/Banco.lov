class Banco(){
    int id;
    int dinheiro;
    string nome;
    float bancovariavel = 1;

    int transferirBanco(self, int valorBanco, Banco destinoBanco){
        dinheiro = dinheiro - valorBanco;
        if (dinheiro < 0){
            return 0;
        }
        dinheiro = dinheiro + valorBanco[0];
        return 1;
    }
}
